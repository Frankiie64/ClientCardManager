@{
    ViewData["Title"] = "Clientes";
}
<div class="container-fluid mt-md-5">
    <div id="TopDivTable" class="col-md-6">
        <a id="btnCrear" type="button" class="btn btn-success"><i class="fa-solid fa-address-card"></i> Nuevo Cliente</a>
    </div>
    <table id="tablaCliente" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0">
        <thead class="main-table">
            <tr>
                <th>Nombre</th>
                <th>Apellido</th>
                <th>Teléfono</th>
                <th>Ocupación</th>
                <th>Tarjetas Asociadas</th>
                <th>Acciones</th>
            </tr>
        </thead>

    </table>
</div>

<div id="modal">

</div>


@section Scripts {
   <script src="~/js/Cliente/Index.js"></script>
}
